{
    "name": "dnonov/json-parser",
    "description": "Thin wrapper around `json_decode` and `json_encode` with exceptions and some handy methods.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dnonov\\JsonParser\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dimitar Nonov",
            "email": "d.nonov@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.46"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DNonov/JSONParser.git"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dnonov\\JsonParser\\Providers\\JsonParserServiceProvider"
            ]
        },
        "aliases": {
            "JSONParser": "Dnonov\\JsonParser\\Facades\\JSONParser"
        }
    }
}
