{
    "name": "dnonov/json-parser",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dnonov\\JsonParser\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dimitar Nonov",
            "email": "d.nonov@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^8.83"
    },
    "require-dev": {
        "pestphp/pest": "^1.23",
        "orchestra/testbench": "^6.46"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dnonov\\JsonParser\\Providers\\JsonParserServiceProvider"
            ]
        },
        "aliases": {
            "JSONParser": "Dnonov\\JsonParser\\Facades\\JSONParser"
        }
    }
}
